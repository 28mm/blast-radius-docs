            "svg_id": "node_3",
            "resource_name": "tcp",
            "type": "azurerm_lb_nat_rule"
        },
        {
            "definition": {
                "resource_group_name": "${azurerm_resource_group.rg.name}",
                "loadbalancer_id": "${azurerm_lb.lb.id}",
                "name": "tcpProbe",
                "protocol": "tcp",
                "port": 80,
                "interval_in_seconds": 5,
                "number_of_probes": 2
            },
            "label": "[root] azurerm_lb_probe.lb_probe",
            "simple_name": "azurerm_lb_probe.lb_probe",
            "group": 20000,
            "svg_id": "node_4",
