{
    "nodes": [
        {
            "type": "azurerm_network_security_group",
            "svg_id": "node_0",
            "resource_name": "security_group",
            "definition": {
                "name": "${var.prefix}-secgrp",
                "location": "${var.location}",
                "resource_group_name": "${azurerm_resource_group.rg.name}",
                "tags": "${var.tags}"
            },
            "group": 20000,
            "label": "[root] azurerm_network_security_group.security_group",
            "simple_name": "azurerm_network_security_group.security_group"
        },
        {
            "type": "azurerm_network_security_rule",
            "svg_id": "node_1",
            "resource_name": "security_rule_rdp",
            "definition": {
                "name": "rdp",
                "priority": 101,
                "direction": "Inbound",
                "access": "Allow",
                "protocol": "Tcp",
                "source_port_range": "*",
                "destination_port_range": "3389",
                "source_address_prefix": "*",
                "destination_address_prefix": "*",
                "resource_group_name": "${azurerm_resource_group.rg.name}",
                "network_security_group_name": "${azurerm_network_security_group.security_group.name}"
            },
            "group": 20000,
            "label": "[root] azurerm_network_security_rule.security_rule_rdp",
            "simple_name": "azurerm_network_security_rule.security_rule_rdp"
        },
        {
            "type": "azurerm_network_security_rule",
            "svg_id": "node_2",
            "resource_name": "security_rule_ssh",
            "definition": {
                "name": "ssh",
                "priority": 102,
                "direction": "Inbound",
                "access": "Allow",
                "protocol": "Tcp",
                "source_port_range": "*",
                "destination_port_range": "22",
                "source_address_prefix": "*",
                "destination_address_prefix": "*",
                "resource_group_name": "${azurerm_resource_group.rg.name}",
                "network_security_group_name": "${azurerm_network_security_group.security_group.name}"
            },
            "group": 20000,
            "label": "[root] azurerm_network_security_rule.security_rule_ssh",
            "simple_name": "azurerm_network_security_rule.security_rule_ssh"
