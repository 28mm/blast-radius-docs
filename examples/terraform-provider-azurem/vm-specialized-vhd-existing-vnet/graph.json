{
    "nodes": [
        {
            "label": "[root] azurerm_network_interface.nic",
            "type": "azurerm_network_interface",
            "simple_name": "azurerm_network_interface.nic",
            "group": 20000,
            "resource_name": "nic",
            "definition": {
                "name": "nic",
                "location": "${var.location}",
                "resource_group_name": "${azurerm_resource_group.rg.name}",
                "ip_configuration": {
                    "name": "ipconfig",
                    "subnet_id": "${var.existing_subnet_id}",
                    "private_ip_address_allocation": "Dynamic",
                    "public_ip_address_id": "${azurerm_public_ip.pip.id}"
                }
            },
            "svg_id": "node_0"
        },
        {
            "label": "[root] azurerm_public_ip.pip",
            "type": "azurerm_public_ip",
            "simple_name": "azurerm_public_ip.pip",
            "group": 20000,
            "resource_name": "pip",
            "definition": {
                "name": "PublicIp",
                "location": "${var.location}",
                "resource_group_name": "${azurerm_resource_group.rg.name}",
                "public_ip_address_allocation": "Dynamic",
                "domain_name_label": "${var.hostname}"
            },
            "svg_id": "node_1"
        },
        {
            "label": "[root] azurerm_resource_group.rg",
            "type": "azurerm_resource_group",
            "simple_name": "azurerm_resource_group.rg",
            "group": 20000,
            "resource_name": "rg",
            "definition": {
                "name": "${var.resource_group}",
                "location": "${var.location}"
            },
            "svg_id": "node_2"
        },
        {
            "label": "[root] azurerm_storage_account.stor",
            "type": "azurerm_storage_account",
            "simple_name": "azurerm_storage_account.stor",
            "group": 20000,
            "resource_name": "stor",
            "definition": {
                "name": "${var.hostname}",
                "resource_group_name": "${azurerm_resource_group.rg.name}",
                "location": "${var.location}",
                "account_tier": "${var.storage_account_tier}",
                "account_replication_type": "${var.storage_replication_type}"
            },
            "svg_id": "node_3"
        },
        {
            "label": "[root] azurerm_virtual_machine.vm",
            "type": "azurerm_virtual_machine",
            "simple_name": "azurerm_virtual_machine.vm",
            "group": 20000,
            "resource_name": "vm",
            "definition": {
                "name": "${var.hostname}",
                "location": "${var.location}",
                "resource_group_name": "${azurerm_resource_group.rg.name}",
                "vm_size": "${var.vm_size}",
                "network_interface_ids": [
                    "${azurerm_network_interface.nic.id}"
                ],
                "storage_os_disk": {
                    "name": "${var.hostname}osdisk1",
                    "image_uri": "${var.os_disk_vhd_uri}",
                    "vhd_uri": "https://${var.existing_storage_acct}.blob.core.windows.net/${var.existing_vnet_resource_group}-vhds/${var.hostname}osdisk.vhd",
                    "os_type": "${var.os_type}",
                    "caching": "ReadWrite",
                    "create_option": "FromImage"
                },
                "os_profile": {
                    "computer_name": "${var.hostname}",
                    "admin_username": "${var.admin_username}",
                    "admin_password": "${var.admin_password}"
                },
                "os_profile_linux_config": {
                    "disable_password_authentication": false
                },
                "boot_diagnostics": {
                    "enabled": true,
                    "storage_uri": "${azurerm_storage_account.stor.primary_blob_endpoint}"
                }
            },
            "svg_id": "node_4"
        },
        {
            "label": "[root] provider.azurerm",
            "type": "provider",
            "simple_name": "provider.azurerm",
            "group": 20000,
            "resource_name": "azurerm",
            "definition": "",
            "svg_id": "node_5"
        },
        {
            "label": "[root] var.existing_subnet_id",
            "type": "var",
            "simple_name": "var.existing_subnet_id",
            "group": 20000,
            "resource_name": "existing_subnet_id",
            "definition": {
                "description": "The id for the existing subnet in the existing virtual network"
            },
            "svg_id": "node_6"
        },
        {
            "label": "[root] var.hostname",
            "type": "var",
            "simple_name": "var.hostname",
            "group": 20000,
            "resource_name": "hostname",
            "definition": {
                "description": "This variable is used in this template to create the domain name label as well as the virtual machine name. Must be unique."
            },
            "svg_id": "node_7"
        },
        {
            "label": "[root] var.location",
            "type": "var",
            "simple_name": "var.location",
            "group": 20000,
            "resource_name": "location",
            "definition": {
                "description": "The location/region where the virtual network resides.",
                "default": "southcentralus"
            },
            "svg_id": "node_8"
        },
        {
            "label": "[root] var.resource_group",
            "type": "var",
            "simple_name": "var.resource_group",
            "group": 20000,
            "resource_name": "resource_group",
            "definition": {
                "description": "Name of the resource group in which to deploy your new Virtual Machine"
            },
            "svg_id": "node_9"
        },
        {
            "label": "[root] var.storage_account_tier",
            "type": "var",
            "simple_name": "var.storage_account_tier",
            "group": 20000,
            "resource_name": "storage_account_tier",
            "definition": {
                "description": "Defines the Tier of storage account to be created. Valid options are Standard and Premium.",
                "default": "Standard"
            },
            "svg_id": "node_10"
        },
        {
            "label": "[root] var.storage_replication_type",
            "type": "var",
            "simple_name": "var.storage_replication_type",
            "group": 20000,
            "resource_name": "storage_replication_type",
            "definition": {
                "description": "Defines the Replication Type to use for this storage account. Valid options include LRS, GRS etc.",
                "default": "GRS"
            },
            "svg_id": "node_11"
        },
        {
            "label": "[root] var.admin_password",
            "type": "var",
            "simple_name": "var.admin_password",
            "group": 20000,
            "resource_name": "admin_password",
            "definition": {
                "description": "administrator password (recommended to disable password auth)"
            },
            "svg_id": "node_12"
        },
        {
            "label": "[root] var.admin_username",
            "type": "var",
            "simple_name": "var.admin_username",
            "group": 20000,
            "resource_name": "admin_username",
            "definition": {
                "description": "administrator user name",
                "default": "vmadmin"
            },
            "svg_id": "node_13"
        },
        {
            "label": "[root] var.existing_storage_acct",
            "type": "var",
            "simple_name": "var.existing_storage_acct",
            "group": 20000,
            "resource_name": "existing_storage_acct",
            "definition": {
                "description": "The name of the storage account in which your existing VHD and image reside"
            },
            "svg_id": "node_14"
        },
        {
            "label": "[root] var.existing_vnet_resource_group",
            "type": "var",
            "simple_name": "var.existing_vnet_resource_group",
            "group": 20000,
            "resource_name": "existing_vnet_resource_group",
            "definition": {
                "description": "Name of the existing resource group in which the existing vnet resides"
            },
            "svg_id": "node_15"
        },
        {
            "label": "[root] var.os_disk_vhd_uri",
            "type": "var",
            "simple_name": "var.os_disk_vhd_uri",
            "group": 20000,
            "resource_name": "os_disk_vhd_uri",
            "definition": {
                "description": "Uri of the existing VHD in ARM standard or premium storage"
            },
            "svg_id": "node_16"
        },
        {
            "label": "[root] var.os_type",
            "type": "var",
            "simple_name": "var.os_type",
            "group": 20000,
            "resource_name": "os_type",
            "definition": {
                "description": "Type of OS on the existing vhd. Allowed values: 'windows' or 'linux'.",
                "default": "linux"
            },
            "svg_id": "node_17"
        },
        {
            "label": "[root] var.vm_size",
            "type": "var",
            "simple_name": "var.vm_size",
            "group": 20000,
            "resource_name": "vm_size",
            "definition": {
                "description": "Specifies the size of the virtual machine.",
                "default": "Standard_DS1_v2"
            },
            "svg_id": "node_18"
        },
        {
            "label": "[root] meta.count-boundary (count boundary fixup)",
            "type": "meta",
            "simple_name": "meta.count-boundary (count boundary fixup)",
            "group": 20000,
            "resource_name": "count-boundary",
            "definition": "",
            "svg_id": "node_19"
        },
        {
            "label": "[root] output.hostname",
            "type": "output",
            "simple_name": "output.hostname",
            "group": 20000,
            "resource_name": "hostname",
            "definition": {
                "value": "${var.hostname}"
            },
            "svg_id": "node_20"
        },
        {
            "label": "[root] output.ssh_command",
            "type": "output",
            "simple_name": "output.ssh_command",
            "group": 20000,
            "resource_name": "ssh_command",
            "definition": {
                "value": "ssh ${var.admin_username}@${azurerm_public_ip.pip.fqdn}"
            },
            "svg_id": "node_21"
        },
        {
            "label": "[root] output.vm_fqdn",
            "type": "output",
            "simple_name": "output.vm_fqdn",
            "group": 20000,
            "resource_name": "vm_fqdn",
            "definition": {
                "value": "${azurerm_public_ip.pip.fqdn}"
            },
            "svg_id": "node_22"
        },
        {
            "label": "[root] var.address_space",
            "type": "var",
            "simple_name": "var.address_space",
            "group": 20000,
            "resource_name": "address_space",
            "definition": {
                "description": "The address space that is used by the virtual network. You can supply more than one address space. Changing this forces a new resource to be created.",
                "default": "10.0.0.0/16"
            },
            "svg_id": "node_23"
        },
        {
            "label": "[root] var.existing_subnet_name",
            "type": "var",
            "simple_name": "var.existing_subnet_name",
            "group": 20000,
            "resource_name": "existing_subnet_name",
            "definition": {
                "description": "The name for the existing subnet in the existing virtual network"
            },
            "svg_id": "node_24"
        },
        {
            "label": "[root] var.existing_virtual_network_name",
            "type": "var",
            "simple_name": "var.existing_virtual_network_name",
            "group": 20000,
            "resource_name": "existing_virtual_network_name",
            "definition": {
                "description": "The name for the existing virtual network"
            },
            "svg_id": "node_25"
        },
        {
            "label": "[root] var.image_offer",
            "type": "var",
            "simple_name": "var.image_offer",
            "group": 20000,
            "resource_name": "image_offer",
            "definition": {
                "description": "the name of the offer (az vm image list)",
                "default": "UbuntuServer"
            },
            "svg_id": "node_26"
        },
        {
            "label": "[root] var.image_publisher",
            "type": "var",
            "simple_name": "var.image_publisher",
            "group": 20000,
            "resource_name": "image_publisher",
            "definition": {
                "description": "name of the publisher of the image (az vm image list)",
                "default": "Canonical"
            },
            "svg_id": "node_27"
        },
        {
            "label": "[root] var.image_sku",
            "type": "var",
            "simple_name": "var.image_sku",
            "group": 20000,
            "resource_name": "image_sku",
            "definition": {
                "description": "image sku to apply (az vm image list)",
                "default": "16.04-LTS"
            },
            "svg_id": "node_28"
        },
        {
            "label": "[root] var.image_version",
            "type": "var",
            "simple_name": "var.image_version",
            "group": 20000,
            "resource_name": "image_version",
            "definition": {
                "description": "version of the image to apply (az vm image list)",
                "default": "latest"
            },
            "svg_id": "node_29"
        },
        {
            "label": "[root] var.subnet_prefix",
            "type": "var",
            "simple_name": "var.subnet_prefix",
            "group": 20000,
            "resource_name": "subnet_prefix",
            "definition": {
                "description": "The address prefix to use for the subnet.",
                "default": "10.0.10.0/24"
            },
            "svg_id": "node_30"
        },
        {
            "label": "[root] provider.azurerm (close)",
            "type": "provider",
            "simple_name": "provider.azurerm (close)",
            "group": 20000,
            "resource_name": "azurerm",
            "definition": "",
            "svg_id": "node_31"
        },
        {
            "label": "[root] root",
            "type": "",
            "simple_name": "root",
            "group": 20000,
            "resource_name": "",
            "definition": "",
            "svg_id": "node_32"
        }
