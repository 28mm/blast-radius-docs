{
    "nodes": [
        {
            "type": "aws_db_instance",
            "simple_name": "aws_db_instance.default",
            "definition": {
                "depends_on": [
                    "aws_security_group.default"
                ],
                "identifier": "${var.identifier}",
                "allocated_storage": "${var.storage}",
                "engine": "${var.engine}",
                "engine_version": "${lookup(var.engine_version, var.engine)}",
                "instance_class": "${var.instance_class}",
                "name": "${var.db_name}",
                "username": "${var.username}",
                "password": "${var.password}",
                "vpc_security_group_ids": [
                    "${aws_security_group.default.id}"
                ],
                "db_subnet_group_name": "${aws_db_subnet_group.default.id}"
            },
            "resource_name": "default",
            "group": 20000,
            "label": "[root] aws_db_instance.default",
            "svg_id": "node_0"
        },
        {
            "type": "aws_db_subnet_group",
            "simple_name": "aws_db_subnet_group.default",
            "definition": {
                "name": "main_subnet_group",
                "description": "Our main group of subnets",
                "subnet_ids": [
                    "${aws_subnet.subnet_1.id}",
                    "${aws_subnet.subnet_2.id}"
                ]
            },
            "resource_name": "default",
            "group": 20000,
            "label": "[root] aws_db_subnet_group.default",
            "svg_id": "node_1"
        },
        {
            "type": "aws_security_group",
            "simple_name": "aws_security_group.default",
            "definition": {
                "name": "main_rds_sg",
                "description": "Allow all inbound traffic",
                "vpc_id": "${var.vpc_id}",
                "ingress": {
                    "from_port": 0,
                    "to_port": 65535,
                    "protocol": "TCP",
                    "cidr_blocks": [
                        "${var.cidr_blocks}"
                    ]
                },
                "egress": {
                    "from_port": 0,
                    "to_port": 0,
                    "protocol": "-1",
                    "cidr_blocks": [
                        "0.0.0.0/0"
                    ]
                },
                "tags": {
                    "Name": "${var.sg_name}"
                }
            },
            "resource_name": "default",
            "group": 20000,
            "label": "[root] aws_security_group.default",
            "svg_id": "node_2"
        },
        {
            "type": "aws_subnet",
            "simple_name": "aws_subnet.subnet_1",
            "definition": {
                "vpc_id": "${var.vpc_id}",
                "cidr_block": "${var.subnet_1_cidr}",
                "availability_zone": "${var.az_1}",
                "tags": {
                    "Name": "main_subnet1"
                }
            },
            "resource_name": "subnet_1",
            "group": 20000,
            "label": "[root] aws_subnet.subnet_1",
            "svg_id": "node_3"
        },
        {
            "type": "aws_subnet",
            "simple_name": "aws_subnet.subnet_2",
            "definition": {
                "vpc_id": "${var.vpc_id}",
                "cidr_block": "${var.subnet_2_cidr}",
                "availability_zone": "${var.az_2}",
                "tags": {
                    "Name": "main_subnet2"
                }
            },
            "resource_name": "subnet_2",
            "group": 20000,
            "label": "[root] aws_subnet.subnet_2",
            "svg_id": "node_4"
        },
        {
            "type": "provider",
            "simple_name": "provider.aws",
            "definition": "",
            "resource_name": "aws",
            "group": 20000,
            "label": "[root] provider.aws",
            "svg_id": "node_5"
        },
        {
            "type": "var",
            "simple_name": "var.db_name",
            "definition": {
                "default": "mydb",
                "description": "db name"
            },
            "resource_name": "db_name",
            "group": 20000,
            "label": "[root] var.db_name",
            "svg_id": "node_6"
        },
        {
            "type": "var",
            "simple_name": "var.engine",
            "definition": {
                "default": "postgres",
                "description": "Engine type, example values mysql, postgres"
            },
            "resource_name": "engine",
            "group": 20000,
            "label": "[root] var.engine",
            "svg_id": "node_7"
        },
        {
            "type": "var",
            "simple_name": "var.engine_version",
            "definition": {
                "description": "Engine version",
                "default": {
                    "mysql": "5.6.22",
                    "postgres": "9.4.1"
                }
            },
            "resource_name": "engine_version",
            "group": 20000,
            "label": "[root] var.engine_version",
            "svg_id": "node_8"
        },
        {
            "type": "var",
            "simple_name": "var.identifier",
            "definition": {
                "default": "mydb-rds",
                "description": "Identifier for your DB"
            },
            "resource_name": "identifier",
            "group": 20000,
            "label": "[root] var.identifier",
            "svg_id": "node_9"
        },
        {
            "type": "var",
            "simple_name": "var.instance_class",
            "definition": {
                "default": "db.t2.micro",
                "description": "Instance class"
            },
            "resource_name": "instance_class",
            "group": 20000,
            "label": "[root] var.instance_class",
            "svg_id": "node_10"
        },
        {
            "type": "var",
            "simple_name": "var.password",
            "definition": {
                "description": "password, provide through your ENV variables"
            },
            "resource_name": "password",
            "group": 20000,
            "label": "[root] var.password",
            "svg_id": "node_11"
        },
        {
            "type": "var",
            "simple_name": "var.storage",
            "definition": {
                "default": "10",
